<li class="control-group nested-fields">
  <div class="controls">
    <%= f.fields_for :exercise do |workout_exercise| %>
      <%= workout_exercise.label :name %>
      <%= workout_exercise.text_field :name %>
    <% end %>

    <%= f.label :sets %>
    <%= f.text_field :sets %>

    <%= f.label :reps %>
    <%= f.text_field :reps %>

    <%= f.label :rest %>
    <%= f.text_field :rest %>

    <%= link_to_remove_association "remove", f %>
  </div>
</li>
